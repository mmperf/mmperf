func @matmul(%A: memref<${M}x${K}xf32>, %B: memref<${K}x${N}xf32>, %C: memref<${M}x${N}xf32>) {

  %f0 = constant 0.0 : f32

  linalg.fill(%f0, %C) : f32, memref<${M}x${N}xf32>

  linalg.matmul ins(%A, %B : memref<${M}x${K}xf32>, memref<${K}x${N}xf32>)
    outs(%C: memref<${M}x${N}xf32>)

  return
}
